{
  "name": "sales-dashboard-backend",
  "version": "1.0.0",
  "description": "Enterprise-grade Sales Performance Dashboard API with real-time analytics and multi-tenant architecture",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "ts-node prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "docker:build": "docker build -t sales-dashboard-api .",
    "docker:run": "docker run -p 8080:8080 sales-dashboard-api"
  },
  "keywords": [
    "sales-analytics",
    "dashboard-api",
    "real-time",
    "typescript",
    "express",
    "multi-tenant",
    "microSaaS"
  ],
  "author": "Sales Performance Dashboard Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "@types/express": "^4.17.21",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "@prisma/client": "^5.7.1",
    "prisma": "^5.7.1",
    "jsonwebtoken": "^9.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "bcryptjs": "^2.4.3",
    "@types/bcryptjs": "^2.4.6",
    "zod": "^3.22.4",
    "cors": "^2.8.5",
    "@types/cors": "^2.8.17",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "redis": "^4.6.12",
    "@types/redis": "^4.0.10",
    "socket.io": "^4.7.4",
    "@types/socket.io": "^3.0.2",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "compression": "^1.7.4",
    "@types/compression": "^1.7.5",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "@types/multer": "^1.4.11",
    "csv-parser": "^3.0.0",
    "@types/csv-parser": "^3.0.0",
    "node-cron": "^3.0.3",
    "@types/node-cron": "^3.0.11",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "axios": "^1.6.2",
    "date-fns": "^3.0.6",
    "uuid": "^9.0.1",
    "@types/uuid": "^9.0.7",
    "joi": "^17.11.0",
    "@types/joi": "^17.2.3",
    "express-async-errors": "^3.1.1",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.3.2",
    "@types/ioredis": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "@types/node": "^20.10.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "prettier": "^3.1.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^6.0.2",
    "jest-mock-extended": "^3.0.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": ["<rootDir>/src", "<rootDir>/tests"],
    "testMatch": ["**/__tests__/**/*.ts", "**/?(*.)+(spec|test).ts"],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/server.ts"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": ["text", "lcov", "html"],
    "setupFilesAfterEnv": ["<rootDir>/tests/setup.ts"]
  }
}