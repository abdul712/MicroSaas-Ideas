{
  "name": "social-proof-conversion-platform",
  "version": "1.0.0",
  "description": "Enterprise-grade social proof widget platform for conversion optimization",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "widget:build": "webpack --config widget/webpack.config.js",
    "widget:dev": "webpack --config widget/webpack.config.js --watch",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.0",
    "@next/font": "^14.1.0",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "prisma": "^5.8.0",
    "@prisma/client": "^5.8.0",
    "next-auth": "^4.24.0",
    "@next-auth/prisma-adapter": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0",
    "@types/bcryptjs": "^2.4.0",
    "@types/jsonwebtoken": "^9.0.0",
    "redis": "^4.6.0",
    "ioredis": "^5.3.0",
    "@types/ioredis": "^5.0.0",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.7.0",
    "@types/socket.io": "^3.0.0",
    "bullmq": "^5.1.0",
    "zod": "^3.22.0",
    "@hookform/resolvers": "^3.3.0",
    "react-hook-form": "^7.48.0",
    "zustand": "^4.4.0",
    "axios": "^1.6.0",
    "date-fns": "^3.0.0",
    "uuid": "^9.0.0",
    "@types/uuid": "^9.0.0",
    "recharts": "^2.10.0",
    "framer-motion": "^10.18.0",
    "react-hot-toast": "^2.4.0",
    "lucide-react": "^0.303.0",
    "@radix-ui/react-dialog": "^1.0.0",
    "@radix-ui/react-dropdown-menu": "^2.0.0",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-switch": "^1.0.0",
