{
  "name": "competitor-analysis-backend",
  "version": "1.0.0",
  "description": "Express.js backend API for competitor analysis platform",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec tsx src/server.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --config jest.e2e.config.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx src/prisma/seed.ts",
    "db:reset": "prisma migrate reset",
    "docker:build": "docker build -t competitor-backend .",
    "docker:run": "docker run -p 4000:4000 competitor-backend"
  },
  "keywords": [
    "express",
    "typescript",
    "competitor-analysis",
    "api",
    "websocket",
    "prisma",
    "postgresql"
  ],
  "author": "Competitor Analysis Platform",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^2.0.1",
    "express-validator": "^7.0.1",
    "socket.io": "^4.7.4",
    "socket.io-redis": "^6.1.1",
    "@prisma/client": "^5.7.1",
    "prisma": "^5.7.1",
    "redis": "^4.6.12",
    "ioredis": "^5.3.2",
    "kafkajs": "^2.2.4",
    "clickhouse": "^2.6.0",
    "elasticsearch": "^16.7.3",
    "@elastic/elasticsearch": "^8.11.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "argon2": "^0.31.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "nodemailer": "^6.9.8",
    "@types/nodemailer": "^6.4.14",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "dotenv": "^16.3.1",
    "joi": "^17.12.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.44",
    "lodash": "^4.17.21",
    "@types/lodash": "^4.14.202",
    "uuid": "^9.0.1",
    "@types/uuid": "^9.0.7",
    "cron": "^3.1.6",
    "axios": "^1.6.5",
    "cheerio": "^1.0.0-rc.12",
    "puppeteer": "^21.6.1",
    "playwright": "^1.40.1",
    "sharp": "^0.33.1",
    "multer": "^1.4.5-lts.1",
    "@types/multer": "^1.4.11",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "pdf-lib": "^1.17.1",
    "jspdf": "^2.5.1",
    "openai": "^4.24.1",
    "stripe": "^14.10.0",
    "twilio": "^4.19.3",
    "@slack/web-api": "^6.10.0",
    "discord.js": "^14.14.1",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "google-auth-library": "^9.4.1",
    "googleapis": "^130.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@types/cheerio": "^0.22.35",
    "@types/jest": "^29.5.11",
    "typescript": "^5.3.3",
    "tsx": "^4.6.2",
    "tsc-alias": "^1.8.8",
    "nodemon": "^3.0.2",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "prettier": "^3.1.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^6.0.2",
    "jest-mock-extended": "^3.0.5",
    "testcontainers": "^10.4.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}