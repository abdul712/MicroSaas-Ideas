{
  "name": "competitor-analysis-platform",
  "version": "1.0.0",
  "description": "Comprehensive competitor analysis and market intelligence SaaS platform",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "dev:backend": "cd backend && npm run dev",
    "dev:scraper": "cd scraper && python -m uvicorn main:app --reload --port 8001",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:backend\" \"npm run dev:scraper\"",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:seed": "npx prisma db seed"
  },
  "dependencies": {
